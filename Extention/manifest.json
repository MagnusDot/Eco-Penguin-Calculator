{
  "name": "Penguin Data Calculator",
  "description": "Calculate you're data consumption with fun Penguin",
  "version": "1.0.0",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "48": "icons/pingoo-on.png"
    },
    "default_popup": "Pages/popup.html"
  },
  "manifest_version": 2,
  "default_locale": "en",
  "options_page": "./Pages/options.html",
  "devtools_page": "GreenIT-Analysis.html",
  "permissions": ["storage", "webRequest", "\u003Call_urls>"]
}
